<div class="container">
    <h1>Novo </h1>

    <form novalidate="novalidate" class="form-horizontal">
        <div class="control-group">
            <label class="control-label">Nome:</label>
            <div class="controls">
                <input type="text" ng-model="imobiliaria.nome"/>
            </div>
        </div>
        <div class="control-group">
            <label class="control-label">CNPJ:</label>
            <div class="controls">
                <input type="text" ng-model="imobiliaria.cnpj"/>
            </div>
        </div>
        <div class="control-group">
            <div class="controls">
                <a ng-click="adicionarImobiliaria()" class="btn btn-small btn-primary">Adicionar</a>
            </div>
        </div>
    </form>
</div>