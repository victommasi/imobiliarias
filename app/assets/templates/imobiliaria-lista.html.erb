<h1> Imobiliarias </h1>
    <div class="span3">
      <table class="table table-striped table-condensed">
        <thead>
          <tr>
            <th style="min-width: 80px;"> Nome: </th>
            <th style="min-width: 80px;"> CNPJ: </th>
          </tr>
        </thead>
        <tbody>
          <tr ng-repeat="imobiliaria in imobiliarias">
            <td> {{imobiliaria.nome}} </td>
            <td> {{imobiliaria.cnpj}} </td>>
            <td><a class="btn btn-small btn-primary" ng-click="editarImobiliaria(imobiliaria.id)">Editar</a></td>
            <td><a class="btn btn-small btn-danger" ng-click="deletarImobiliaria(imobiliaria.id)">Deletar</a></td>
          </tr>
        </tbody>
      </table>
      <a class="btn btn-large btn-info" ng-click="novaImobiliaria()"> Novo </a>
    </div>